# Database connection
DB_HOST=your-aiven-host.aivencloud.com
DB_PORT=3306
DB_USERNAME=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_db_name

# Aiven SSL: either paste certificate PEM into AIVEN_SSL_CA, or provide a path
AIVEN_SSL_CA_PATH=path-to-your-aiven-ca.pem

# JWT configuration
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=1h

# Application
PORT=8000
NODE_ENV=development
